<powerdba-lookupgroup-list>

	<display-groups>
		
	  <display-group name="sessions">
	  	
	  	<lookups>
			
			<lookup name="sessionstatus">
				<label> Status </label>
				<display-priority> 1 </display-priority>
				<select-entries>
					<select-entry>
						<value> % </value>
						<display> ANY </display>
						<order> 1 </order>
					</select-entry>
					<select-entry>
						<value> ACTIVE </value>
						<display> ACTIVE </display>
						<order> 2 </order>
					</select-entry>
					<select-entry>
						<value> INACTIVE </value>
						<display> INACTIVE </display>
						<order> 3 </order>
					</select-entry>
				</select-entries>
				<initial-value> ACTIVE </initial-value>
			</lookup>
			
			<lookup name="oracleuser">
				<label> User </label>
				<display-priority> 2 </display-priority>
				<select-entries>
					<select-entry>
						<value> % </value>
						<display> ANY </display>
						<order> -99 </order>
					</select-entry>
				</select-entries>
			</lookup>
			
			<lookup name="service">
				<label> Service </label>
				<display-priority> 3 </display-priority>
				<select-entries>
					<select-entry>
						<value> % </value>
						<display> ANY </display>
						<order> -99 </order>
					</select-entry>
				</select-entries>
			</lookup>
			
			<lookup name="module">
				<label> Module </label>
				<display-priority> 4 </display-priority>
				<select-entries>
					<select-entry>
						<value> % </value>
						<display> ANY </display>
						<order> -99 </order>
					</select-entry>
				</select-entries>
			</lookup>
			
			<lookup name="machine">
				<label> Machine </label>
				<display-priority> 5 </display-priority>
				<select-entries>
					<select-entry>
						<value> % </value>
						<display> ANY </display>
						<order> -99 </order>
					</select-entry>
				</select-entries>
			</lookup>

			<lookup name="statistic">
				<label> Statistic </label>
				<display-priority> 6 </display-priority>
				<initial-value> 9 </initial-value>
			</lookup>
		
		</lookups>
		
	  </display-group>
	  
	  <display-group name="locks">	  	
	  	<lookups>
	  		<lookup name="sessiontype">
	  			<label> Session Type </label>
